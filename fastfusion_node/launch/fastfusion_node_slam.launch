<?xml version="1.0" encoding="ISO-8859-15"?>
<launch>
	<arg name="slam_name" value="/picoflexx/cam0"/>
	
	<!-- Run Bagfile -->
     <node pkg="rosbag" type="play" name="player" output="screen"   
      args="-r 1.0 /home/karrer/Desktop/Dataset3/2016_Jan_09/Picoflexx/Desk/Mode1/desk_mode1_run1.bag" />
      
	<!-- Run rovio -->
     <node  name="rovio" pkg="rovio" type="rovio_node"     
     clear_params="true" output="screen" > 
     </node>


	<!-- Run fastfusion_node-->

	<node  name="fastfusion_node" pkg="fastfusion_node" type="fastfusion_node" clear_params="true" output="screen" >
		<remap from="image_depth"		to="$(arg slam_name)/depth/image_raw" />
		<remap from="image_conf"		to="$(arg slam_name)/confidence/image_raw" />
		<remap from="image_noise" 		to="$(arg slam_name)/noise/image_raw" />

	<!-- Load parameters -->
 	<rosparam file="$(find fastfusion_node)/cfg/params.yaml"/> 
 	<rosparam file="$(find fastfusion_node)/cfg/camchain_pico.yaml"/>
	</node>
	
	
	<!--node-->
</launch>
